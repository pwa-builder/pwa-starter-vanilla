{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:prod": "rimraf dist/ && tsc && rollup -c",
    "start": "es-dev-server --app-index index.html --node-resolve --watch --open",
    "dev": "concurrently \"tsc --watch\" \"es-dev-server --app-index index.html --node-resolve --watch --open\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pwabuilder/pwainstall": "^1.4.2",
    "@vaadin/router": "^1.7.1",
    "lit-element": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-strip": "^1.3.2",
    "concurrently": "^5.1.0",
    "es-dev-server": "^1.45.2",
    "lit-analyzer": "^1.1.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.1.0",
    "rollup-plugin-babel-minify": "^10.0.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-minify-html-literals": "^1.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-workbox": "^5.0.1",
    "typescript": "^3.8.3"
  }
}
